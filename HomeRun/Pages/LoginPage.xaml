<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HomeRun.Pages.LoginPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry Keyboard="Email" Placeholder="User" x:Name="UserLoginEmail" />
            <Entry IsPassword="True" Placeholder="Password" x:Name="UserLoginPassword" />
            <Button Text="Login" Clicked="Button_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>